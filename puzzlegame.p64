function drawCheckerBoard(offset_x, offset_y, grid_w, grid_h, clr, bg_clr)
	--checkerboard
	rectfill(offset_x, offset_y, offset_x + (grid_w * grid_size) - 1, offset_y + (grid_h * grid_size) - 1, bg_clr)
	for x = 0, grid_w - 1 do
